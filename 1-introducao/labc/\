#include <stdio.h>
#include <stdlib.h>

int main(){
	int vetor[5] = {3,2,2,4,2};
	int achar;
	printf("{");
	for(int i = 0; i < 5; i++){
		printf(" %d, ",vetor[i]); 
	}
	printf(" }\n");
	printf("digite o elemento que queira tirar: ");
	scanf("%d", &achar);
	
	printf("{ ");
	for(int i = 0;i < 5; i++){
		if(vetor[i] == achar){
			continue;
		}
		printf(" %d, ",vetor[i]);
	}
	printf(" }\n");

}
